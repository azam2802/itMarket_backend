{
    "builds": [
        {
            "src": "project/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            },
            "api/**": {
                "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "project/wsgi.py"
        }
    ],
}